{
	"streams": [
		"generators.RandomRBFGenerator",
		"generators.WaveformGenerator"
	],
	"filters": [
		{
			"filter": "differentialprivacy.DifferentialPrivacyFilter",
			"params": [
				{
					"name": "k",
					"values": [
						3, 10, 50, 100
					]
				},
				{
					"name": "e",
					"values": [
						0.01, 0.1, 1, 10, 100
					]
				},
				{
					"name": "b",
					"values": [
						100000
					]
				}
			]
		}
	],
	"options": {
		"silenceTaskReport": false,
		"taskReportOutputDirectory": "report",
		"silenceAnonymization": true,
		"anonymizationOutputDirectory": "streams/anonymous",
		"suppressAnonymizationHeader": false,
		"maximumInstances": 10000000,
		"silenceEvaluation": false,
		"evaluationOutputDirectory": "evaluation",
		"evaluationUpdateRate": 1000
	}
}
